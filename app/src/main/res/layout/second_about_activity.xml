<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.chinayszc.mobile.widget.CommonTitleView
        android:id="@+id/second_about_title"
        android:layout_width="match_parent"
        android:layout_height="@dimen/title_bar_height"/>

    <FrameLayout
        android:id="@+id/me_about_yisheng"
        android:layout_width="match_parent"
        android:layout_height="@dimen/me_item_height"
        android:background="@drawable/me_item_selector">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="@dimen/me_item_height"
            android:layout_gravity="left"
            android:layout_marginLeft="@dimen/app_margin_left"
            android:gravity="center_vertical"
            android:text="@string/about_yisheng"
            android:textColor="@color/text_dark"
            android:textSize="@dimen/text_middle" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="@dimen/me_item_height"
            android:layout_gravity="right"
            android:layout_marginRight="12dp"
            android:contentDescription="into icon"
            android:paddingBottom="15dp"
            android:paddingTop="15dp"
            android:scaleType="centerInside"
            android:src="@mipmap/into_icon" />

    </FrameLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/line_color" />

    <FrameLayout
        android:id="@+id/me_feed_back"
        android:layout_width="match_parent"
        android:layout_height="@dimen/me_item_height"
        android:background="@drawable/me_item_selector">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="@dimen/me_item_height"
            android:layout_gravity="left"
            android:layout_marginLeft="@dimen/app_margin_left"
            android:gravity="center_vertical"
            android:text="@string/feed_back"
            android:textColor="@color/text_dark"
            android:textSize="@dimen/text_middle" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="@dimen/me_item_height"
            android:layout_gravity="right"
            android:layout_marginRight="12dp"
            android:contentDescription="into icon"
            android:paddingBottom="15dp"
            android:paddingTop="15dp"
            android:scaleType="centerInside"
            android:src="@mipmap/into_icon" />

    </FrameLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/line_color" />

    <TextView
        android:id="@+id/me_phone"
        android:layout_width="match_parent"
        android:layout_height="@dimen/me_item_height"
        android:layout_gravity="left"
        android:background="@color/white"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/app_margin_left"
        android:text="@string/client_phone"
        android:textColor="@color/text_dark"
        android:textSize="@dimen/text_middle" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/line_color" />

    <TextView
        android:id="@+id/me_version"
        android:layout_width="match_parent"
        android:layout_height="@dimen/me_item_height"
        android:layout_gravity="left"
        android:background="@color/white"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/app_margin_left"
        android:text="@string/version"
        android:textColor="@color/text_dark"
        android:textSize="@dimen/text_middle" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/line_color" />


</LinearLayout>